        <name>Validate Date to Return the Vehicle</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

   //Type appropriate comment here, and begin script below

   (function executeRule(current, gForm, gS) {
    var dateNeeded = gForm.getValue('date_needed'); // Get Date Needed
    var dateToReturnTheVehicle = gForm.getValue('date_to_return_the_vehicle'); // Get Date to Return

    if (dateNeeded && dateToReturnTheVehicle) {
        var neededDate = new Date(dateNeeded);
        var returnDate = new Date(dateToReturnTheVehicle);

        if (returnDate < neededDate) {
            gForm.clearValue('date_to_return_the_vehicle'); // Clear invalid date
            gForm.addErrorMessage('Date to return should not be before the Date needed');
        }
    }
})(current, gForm, gS);

   
}]]></script>
