        <name>Vehicle List Widget</name>
        <option_schema/>
        <public>false</public>
        <roles/>
        <script><![CDATA[(function() {
  /* populate the 'data' object */
  /* e.g., data.table = $sp.getValue('table'); */

  var gr = new GlideRecord('x_1538165_lva_loaner_vehicle_catalog');
    gr.query();

    var vehicles = [];
    while (gr.next()) {
        vehicles.push({
            name: gr.name.toString(),
            status: gr.status.toString(),
            location: gr.location.toString(),
            assigned_to: gr.assigned_to.getDisplayValue()
        });
    }

    data.vehicles = vehicles;

})();]]></script>
