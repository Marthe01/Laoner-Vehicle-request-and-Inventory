        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

        // Add your code here

    var vehStatus = current.vehicle_status;
    var loanerGr = new GlideRecord('x_1538165_lva_loaner_vehicle_catalog'); 
    
    if (loanerGr.get(current.vehicle)) { // Fetche the vehicle record

	
        
        // Map Vehicle Tracker status to Vehicle status
        if (vehStatus == 'pending_release' || vehStatus == 'out_on_field' || vehStatus == 'returned_for_inspection' || vehStatus == 'sent_for_servicing') {
            loanerGr.status = 'unavailable';
        } else if (vehStatus == 'returned_to_warehouse') {
            loanerGr.status = 'available'; 
        } else if (vehStatus == 'decommissioned') {
            loanerGr.status = 'decommissioned';
        }

        loanerGr.update();
    }
		
})(current, previous);]]></script>
