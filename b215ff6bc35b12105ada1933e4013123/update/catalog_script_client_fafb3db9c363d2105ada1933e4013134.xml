        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

   //Type appropriate comment here, and begin script below
	var dateToReturnTheVehicle = g_form.getValue('date_to_return_the_vehicle');
	var dateNeeded = g_form.getValue('date_needed'); 
	

//If date return is before date needed trigger error message 
	if (dateToReturnTheVehicle < dateNeeded) {
		g_form.addErrorMessage('Date to return the vehicle should not be before the Date needed');
		g_form.clearValue('date_to_return_the_vehicle');
		return false; // Prevent form submission

	}

	return true; // Allow form submission
   
}

