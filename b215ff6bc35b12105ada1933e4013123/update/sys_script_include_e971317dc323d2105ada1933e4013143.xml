        <script><![CDATA[var GetUserLocation = Class.create();
GetUserLocation.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
    
    GetUserLocation: function() {
        var userid = this.getParameter('sysparm_user_sysid'); // Get user ID from client
        var location = '';

        var gr = new GlideRecord('sys_user');
        gr.addQuery('sys_id', userid);
        gr.query();
         
            if (gr.next()) {
                location = gr.getValue('location'); // Get Office Location
            }
        
        
        return location; // Return location or empty if not found
    }

});



]]></script>
